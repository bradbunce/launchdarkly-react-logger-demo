# Node environment (development/production)
VITE_NODE_ENV=development

# LaunchDarkly client-side ID
VITE_LD_CLIENT_ID=your-client-side-id

# Application name for LaunchDarkly context
VITE_APP_NAME=launchdarkly-react-logger-demo

# Feature flag key for controlling SDK log levels (string type: 'error', 'warn', 'info', 'debug')
VITE_LD_SDK_LOG_FLAG_KEY=your-sdk-log-flag-key

# Feature flag key for controlling console log levels (number type: 0-5)
VITE_LD_CONSOLE_LOG_FLAG_KEY=your-console-log-flag-key